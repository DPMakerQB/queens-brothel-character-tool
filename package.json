{
  "name": "qb-character-tool",
  "version": "1.0.1",
  "description": "Exports character layers from PSD files, for Queen's Brothel.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-windows": "electron-forge make --platform win32 --targets @electron-forge/maker-zip",
    "make-mac": "electron-forge make --platform darwin --targets @electron-forge/maker-dmg",
    "make-linux": "electron-forge make --platform linux --targets @electron-forge/maker-zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DPMakerQB/queens-brothel-character-tool.git"
  },
  "author": "DPMaker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DPMakerQB/queens-brothel-character-tool/issues"
  },
  "homepage": "https://github.com/DPMakerQB/queens-brothel-character-tool#readme",
  "dependencies": {
    "jimp": "^0.16.1",
    "psd": "^3.4.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-dmg": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "electron": "^19.0.7"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  }
}
